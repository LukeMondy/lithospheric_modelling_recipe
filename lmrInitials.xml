<?xml version="1.0" encoding="UTF-8"?>
<StGermainData xmlns="http://www.vpac.org/StGermain/XML_IO_Handler/Jun2003">
    
    <param name="TemperatureProfile2StartCoord" units="km">   0.0</param>
    <param name="TemperatureProfile2EndCoord"   units="km">-100.0</param> <!-- Avoid using references (such as minY) in this function -->
    <param name="TemperatureProfile2MinTemp" units="K">293.15</param>
    <param name="TemperatureProfile2MaxTemp" units="K">1473.00</param>
    <param name="TemperatureProfile2LinearCoefficient">4.3e-3</param> 
    <param name="TemperatureProfile2ExponentialCoefficient1">920.0 </param> 
    <param name="TemperatureProfile2ExponentialCoefficient2">3.5e-5</param> 
    
    
    <struct name="temperatureICs">
        <param name="type">CompositeVC</param>
        <list name="vcList">
            <!-- Note: each struct is applied in order. If you put a MeshShapeVC first, followed by 
                 a AllNodesVC, the MeshShapeVC will be overwritten. -->
            <struct>
                <param name="type"> AllNodesVC </param>
                <list name="variables">
                    <struct>
                        <param name="name">temperature</param>
                        <param name="type">func</param>
                        <param name="value"> TemperatureProfile2 </param>
                    </struct>
                </list>
            </struct>
            
            <!-- Note: Using a MeshShapeVC type can force a temperature in a particular area.
                 For example: 
                    <struct>
                        <param name="type"> MeshShapeVC </param>
                        <param name="Shape"> airShape </param>
                        <list name="variables">
                            <struct>
                                <param name="name">temperature</param>
                                <param name="type">double</param>
                                <param name="value" units="K">  273.15 </param>
                            </struct>
                        </list>
                    </struct>
            -->
            
        </list>
    </struct>
    
</StGermainData>
