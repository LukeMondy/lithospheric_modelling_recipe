            <param name="ActivationEnergy" units="J/mol">520000</param>
            <param name="ActivationVolume">0.0</param>
            
            <param name="minViscosity" units="Pa*s">5e18</param> 
            <param name="maxViscosity" units="Pa*s">5e23</param> 
        </struct>
        
        <!-- Wet Dunite : Brace and Kohlstedt, JGR 1980: PreExponentialFactor*100 => weaker-->
        <struct name="asthenoViscosity">
            <param name="Type">ViscousCreep</param>
            <param name="StrainRateInvariantField">recoveredStrainRateInvariantField</param>
            <param name="TemperatureField">TemperatureField</param>
